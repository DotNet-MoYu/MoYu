{
  "$schema": "http://json.schemastore.org/template",
  "author": "少林寺驻北固山办事处大神父王喇嘛",
  "classifications": ["MoYu", "WebApi", "Api", "RESTful", "Swagger"],
  "name": "MoYu | 规范化接口Api",
  "identity": "MoYu.Template.Api",
  "shortName": "moyuapi",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MoYuApi",
  "preferNameDirectory": true,
  "symbols": {
    "framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net5",
          "description": ".NET5 脚手架"
        },
        {
          "choice": "net6",
          "description": ".NET6 脚手架"
        },
        {
          "choice": "net7",
          "description": ".NET7 脚手架"
        },
        {
          "choice": "net8",
          "description": ".NET8 脚手架"
        },
        {
          "choice": "net9",
          "description": ".NET9 脚手架"
        }
      ],
      "defaultValue": "net8",
      "description": "选择要创建的 .NET5-9 脚手架"
    },
    "net5": {
      "type": "computed",
      "value": "(framework == \"net5\")"
    },
    "net6": {
      "type": "computed",
      "value": "(framework == \"net6\")"
    },
    "net7": {
      "type": "computed",
      "value": "(framework == \"net7\")"
    },
    "net8": {
      "type": "computed",
      "value": "(framework == \"net8\")"
    },
    "net9": {
      "type": "computed",
      "value": "(framework == \"net9\")"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(framework != \"net5\")",
          "exclude": ["MoYuApi.Web.Entry/Startup.cs"]
        }
      ]
    }
  ]
}
